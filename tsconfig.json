{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "lib": ["ESNext","DOM"],
    "allowJs": false,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "removeComments": false,
    "resolveJsonModule": true,
  }
}
